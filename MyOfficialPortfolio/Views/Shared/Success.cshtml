@using MyOfficialPortfolio.Models.OutputModels

@{
    ViewBag.Title = "Success";
    SuccessModel successModel = ViewBag.SuccessModel;
}

<h1>Success!</h1>

<p>
    @successModel.Message
</p>
<p>
    @foreach (var link in successModel.ActionLinks)
    {
        <a class="btn btn-primary" href="@link.URL">@link.Name</a>
    }

    <a asp-controller="Home" asp-action="Index">Back to Home</a>
</p>